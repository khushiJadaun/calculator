<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Nine"
    android:background="@drawable/calcibg"
    >

    <EditText
        android:id="@+id/editTextText10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="19dp"
        android:layout_marginTop="44dp"
        android:backgroundTint="#82B1FF"
        android:ems="10"
        android:hint="enter your 1st number"
        android:inputType="text"
        android:textColor="#26A69A"
        android:textSize="20sp"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/editTextText11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="20dp"
        android:layout_marginTop="130dp"
        android:backgroundTint="#7C4DFF"
        android:ems="10"
        android:hint="enter your 2nd number"
        android:inputType="text"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button12"
        android:layout_width="68dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="61dp"
        android:layout_marginBottom="385dp"
        android:backgroundTint="#C8E6C9"
        android:text="Add"
        android:textColor="#FF4081"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button13"
        android:layout_width="59dp"
        android:layout_height="47dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="237dp"
        android:layout_marginBottom="282dp"
        android:backgroundTint="#B2DFDB"
        android:text="Sub"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button14"
        android:layout_width="65dp"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="64dp"
        android:layout_marginBottom="177dp"
        android:backgroundTint="#FFCCBC"
        android:text="Mul"
        android:textColor="#2979FF"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button15"
        android:layout_width="62dp"
        android:layout_height="49dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="125dp"
        android:layout_marginBottom="62dp"
        android:backgroundTint="#FFF59D"
        android:text="Div"
        android:textColor="#D500F9"
        android:textSize="20sp"
        android:textStyle="bold" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="75dp"
        android:layout_marginBottom="481dp"
        android:text="result is"
        android:textColor="#DD2C00"
        android:textSize="20sp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/button16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="143dp"
        android:layout_marginBottom="6dp"
        android:text="back" />
</RelativeLayout>